<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>AbandonProcessGroup</key>
	<true/>
	<key>Label</key>
	<string>com.tjluoma.growl-battery-warning</string>
	<key>Program</key>
	<string>/usr/local/bin/growl-battery-warning.sh</string>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>/tmp/com.tjluoma.growl-battery-warning.log</string>
	<key>StandardOutPath</key>
	<string>/tmp/com.tjluoma.growl-battery-warning.log</string>
	<key>StartInterval</key>
	<integer>60</integer>
</dict>
</plist>
